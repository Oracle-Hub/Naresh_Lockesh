,       s.machine
,       s.username
,       s.osuser
,       s.status
,       p.spid "OS PID"
,       s.program
,	  s.inst_id
from    gv$session s
,       gv$process p
Where   s.paddr = p.addr and  s.status='INACTIVE'
order   by s.inst_id
/

