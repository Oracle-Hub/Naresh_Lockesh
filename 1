col "SID/SERIAL" format a15
col username format a25
col osuser format a15
col program format a50
set line 200
select  distinct s.sid || ',' || s.serial# "SID/SERIAL"
,       s.machine
,       s.username
,       s.osuser
,       s.status
,       p.spid "OS PID"
,       s.program
,	  s.inst_id
from    gv$session s
,       gv$process p
Where   s.paddr = p.addr and s.sid in ('97')
-- and s.PROGRAM like 'sqlplus%'
order   by s.inst_id
/

